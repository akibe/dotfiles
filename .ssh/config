# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host github.com-akibe
  HostName github.com
  User git
  IdentityFile ~/.ssh/akibe
  AddKeysToAgent yes
  UseKeychain yes
  IdentitiesOnly yes

Host github.com-artcodec
  HostName github.com
  User git
  IdentityFile ~/.ssh/artcodec
  AddKeysToAgent yes
  UseKeychain yes
  IdentitiesOnly yes